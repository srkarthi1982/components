---
interface Props {
  /** Extra classes for customization */
  className?: string;

  /** Additional HTML attributes */
  [attrs: string]: any;
}

const {
  className = "",
  ...attrs
} = Astro.props as Props;
const classes = ["av-divider"];
if (className) classes.push(className);
const classAttr = classes.join(" ");
---

<div class={classAttr} {...attrs} />
